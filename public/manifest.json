{
  "name": "BookMark-Extension",
  "description": "BookMark Chrome Extension",
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": ["bookmarks"],
  "action": {
    "default_title": "BookMark",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ]
}
