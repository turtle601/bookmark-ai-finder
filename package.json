{
  "name": "bookmark-board",
  "version": "1.1.0",
  "description": "chrome extension for bookmark helper like MacOS finder",
  "author": "Hwnag Jun Seung, turtle601",
  "keywords": [
    "bookmark",
    "extension",
    "chrome"
  ],
  "scripts": {
    "dev": "webpack serve --mode development",
    "prebuild": "rimraf dist",
    "build": "webpack build --config ./webpack.config.js --mode production",
    "lint::check": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint::fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "prettier::check": "prettier src/**/* --check",
    "prettier::fix": "prettier src/**/* --write --cache",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@chromatic-com/storybook": "^1.6.0",
    "@emotion/babel-preset-css-prop": "^11.11.0",
    "@storybook/addon-essentials": "^8.1.11",
    "@storybook/addon-interactions": "^8.1.11",
    "@storybook/addon-links": "^8.1.11",
    "@storybook/addon-onboarding": "^8.1.11",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.4",
    "@storybook/blocks": "^8.1.11",
    "@storybook/react": "^8.1.11",
    "@storybook/react-webpack5": "^8.1.11",
    "@storybook/test": "^8.1.11",
    "@svgr/webpack": "^8.1.0",
    "@turtle601/eslint-config": "^1.0.0",
    "@turtle601/prettier-config": "^1.0.0",
    "@types/chrome": "^0.0.268",
    "@types/node": "^20.12.12",
    "@types/react-dom": "^18.3.0",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^12.0.2",
    "eslint": "^8.57.0",
    "eslint-plugin-storybook": "^0.8.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "html-webpack-plugin": "^5.6.0",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.7",
    "storybook": "^8.1.11",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "prettier": "@turtle601/prettier-config"
}
